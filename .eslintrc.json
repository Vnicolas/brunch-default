{
    "env": {
        "browser": true
    },
    "globals": {
        "$": true,
        "module": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "semi": 2,
        "no-var": 2,
        "no-console": 1,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-extra-semi": 2,
        "no-unsafe-finally": 2,
        "no-unreachable": 2,
        "complexity": ["error", { "max": 10 }],
        "default-case": 2,
        "eqeqeq": 2,
        "no-alert": 2,
        "no-else-return": 2,
        "no-empty-function": 2,
        "no-eval": 2,
        "no-global-assign": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 2,
        "no-new-func": 2,
        "no-void": 2,
        "no-with": 2,
        "no-undefined": 2,
        "block-spacing": 2,
        "camelcase": 2,
        "indent": ["error", 4],
        "eol-last": ["error", "always"],
        "max-len": [1, 120],
        "max-params": ["error", 4],
        "new-cap": 2,
        "no-array-constructor": 2,
        "no-inline-comments": 2,
        "no-multiple-empty-lines": 2,
        "no-new-object": 2,
        "no-trailing-spaces": 2,
        "no-whitespace-before-property": 2,
        "no-confusing-arrow": 2,
        "prefer-template": 2
    }
}
